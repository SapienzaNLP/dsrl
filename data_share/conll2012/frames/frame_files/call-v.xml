    <predicate lemma="call">
        
        
        <roleset framnet="" id="call.01" name="label" vncls="29.3">
            <roles>
                <role descr="caller" f="" n="0">
                    <vnrole vncls="29.3" vntheta="agent"/>
                </role>
                <role descr="item being labelled" f="" n="1">
                    <vnrole vncls="29.3" vntheta="theme"/>
                </role>
                <role descr="attribute of arg1" f="" n="2">
                    <vnrole vncls="29.3" vntheta="predicate"/>
                </role>
                
                
                
                
                <note>Some of the trees only include arg1 and arg2 under a single S node. Contrary to regular rules, it is ok in these cases to dig into the syntax and label arg1 and arg2 separately. Don't try to cram them into a single argument.</note>
            </roles>
            
            <example name="with attribute" src="" type="">
                <text>Mary called John an idiot</text>
                <arg f="" n="0">Mary</arg>
                <rel f="">called</rel>
                <arg f="" n="1">John</arg>
                <arg f="" n="2">an idiot</arg>
                
                
                
                
                <note/>
            </example>
            
            <example name="passivized" src="" type="">
                <text>The worksheets were called 'Learning Materials'</text>
                <arg f="" n="1">The worksheets</arg>
                <rel f="">called</rel>
                <arg f="" n="2">'Learning Materials'</arg>
                
                
                
                
                <note/>
            </example>
            
            
            
            
            <note/>
        </roleset>
        
