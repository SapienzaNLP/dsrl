                <role descr="agent or cause of decompression" f="" n="0">
                    <vnrole vncls="other_cos-45.4" vntheta="agent"/>
                </role>
                
                <role descr="entity in which the pressure is being reduced" f="" n="1">
                    <vnrole vncls="other_cos-45.4" vntheta="patient"/>
                </role>
                <role descr="tool used to aid in decompression" f="" n="2">
                    <vnrole vncls="other_cos-45.4" vntheta="instrument"/>
                </role>
                
                <note/>
            </roles>
            
            <example name="All args" src="All args" type="All args">
                <inflection aspect="ns" form="full" person="third" tense="past" voice="active"/>
                <text>He decompressed the skull successfully with a carpenter's rusty drill and a wooden brace.</text>
                
                <arg f="" n="0">He</arg>
                <rel f="">decompressed</rel>
                <arg f="" n="1">the skull</arg>
                <arg f="mnr" n="m">successfully</arg>
                <arg f="" n="2">with a carpenter's rusty drill and a wooden brace.</arg>
                
                <note/>
            </example>
            <example name="Cause as subject" src="Cause as subject" type="Cause as subject">
                <inflection aspect="ns" form="full" person="third" tense="past" voice="active"/>
                <text>Two tubular tire sidewall gashes abruptly decompressed the tire.</text>
                
                <arg f="" n="0">Two tubular tire sidewall gashes</arg>
                <arg f="mnr" n="m">abruptly</arg>
                <rel f="">decompressed</rel>
                <arg f="" n="1">the tire.</arg>
                
                <note/>
            </example>
            <example name="Patient as subject" src="Patient as subject" type="Patient as subject">
                <inflection aspect="ns" form="full" person="third" tense="past" voice="active"/>
                <text>My ear just decompressed.</text>
                
                <arg f="" n="1">My ear</arg>
                <arg f="tmp" n="m">just</arg>
                <rel f="">decompressed.</rel>
                
                <note/>
            </example>
            
            <note>Roleset based on internet search. Added because none existed. Vncls 45.4. Comparison to ‘compress’. Roleset by Julia.</note>
        </roleset>
        <roleset framnet="-" id="decompress.03" name="relax/recover from an affliction" vncls="-">
            <roles>
                <role descr="entity recovering from an affliction" f="" n="1">
                    <vnrole vncls="-" vntheta="experiencer"/>
                </role>
                
                <role descr="the affliction" f="" n="2">
                    <vnrole vncls="-" vntheta="stimulus"/>
                </role>
                
                <note/>
            </roles>
            
            <example name="Both args" src="Both args" type="Both args">
                <inflection aspect="ns" form="gerund" person="other" tense="past" voice="active"/>
                <text>I spent the afternoon *PRO* decompressing from yesterday's big-fat-Italian wedding!</text>
                
                <arg f="" n="1">*PRO*</arg>
                <rel f="">decompressing</rel>
                <arg f="" n="2">from yesterday's big-fat-Italian wedding!</arg>
                
                <note/>
            </example>
            <example name="Intransitive without affliction" src="Intransitive without affliction" type="Intransitive without affliction">
                <inflection aspect="ns" form="full" person="third" tense="present" voice="active"/>
                <text>Our thank-you for this is Rough Rider, which will be baked as soon as Adam decompresses.</text>
                
                <arg f="" n="1">Adam</arg>
                <rel f="">decompresses.</rel>
                
                <note/>
            </example>
            
            <note>Roleset based on internet search. Added because none existed. No Vncls. Comparison to ‘recover’. Roleset by Julia.</note>
        </roleset>
        
