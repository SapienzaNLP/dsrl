                    <vnrole vncls="51.1" vntheta="location"/>
                    <vnrole vncls="10.11" vntheta="source"/>
                </role>
                <role descr="attribute of arg1" f="" n="2"/>
                
                
                <note/>
            </roles>
            
            <example name="intransitive" src="" type="">
                <text>John left.</text>
                <arg f="" n="0">John</arg>
                <rel f="">left</rel>
                
                
