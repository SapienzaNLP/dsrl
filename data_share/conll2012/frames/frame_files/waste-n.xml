                    <vnrole vncls="66" vntheta="agent"/>
                    <vnrole vncls="44" vntheta="agent"/>
                    <vnrole vncls="68" vntheta="agent"/>
                </role>
                <note>Arg2 in this frame differs from the verbal frame in that it is not the instrument, but what arg1 is wasted on.</note>
                <role descr="thing wasted" f="ppt" n="1">
                    <vnrole vncls="66" vntheta="theme"/>
                    <vnrole vncls="44" vntheta="patient"/>
                    <vnrole vncls="68" vntheta="asset"/>
