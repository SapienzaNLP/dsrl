                <role descr="what arg1 is wasted on" f="gol" n="2">
                    <vnrole vncls="66" vntheta="recipient"/>
                    <vnrole vncls="68" vntheta="theme"/>
                </role>
            </roles>
            <note>Added by Julia based on a combination of waste-v.01 and the consume-66 and pay-68 VN classes. Corresponds to Vncls consume-66-1, destroy-44, pay-68.</note>
            <example name="just arg1" src="" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>Her husband said voting was a waste of time .</text>
                <note/>
                <rel f="">waste</rel>
                <arg f="" n="1">of time</arg>
            </example>
            <example name="just arg0" src="" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
