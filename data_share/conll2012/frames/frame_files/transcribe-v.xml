<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
    <predicate lemma="transcribe">
        
        
        <roleset framnet="-" id="transcribe.01" name="to make a copy or record on tape" vncls="25.4">
            <roles>
                <role descr="copier, agent" f="" n="0">
                    <vnrole vncls="25.4" vntheta="agent"/>
                </role>
                <role descr="thing copied" f="" n="1">
                    <vnrole vncls="25.4" vntheta="theme"/>
                </role>
                <note/>
