<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
    <predicate lemma="stimulate">
        
        
        <roleset framnet="Cause_to_start , Experiencer_obj" id="stimulate.01" name="cause an increase (in interest, etc)" vncls="31.1  59">
            <roles>
                <role descr="cause of increase" f="" n="0">
                    <vnrole vncls="31.1" vntheta="cause"/>
                    <vnrole vncls="59" vntheta="agent"/>
                </role>
                <role descr="thing increasing" f="ppt" n="1">
                    <vnrole vncls="31.1" vntheta="experiencer"/>
