                <role descr="instrumental" f="" n="4"/>
                
                
                <note/>
            </roles>
            
            <example name="Example with arg 1 as procede" src="" type="">
                <text>prevent Martin Ackerman from making a run at the
                    computer-services concern
                </text>
                
                
                <rel f="">run</rel>
                <arg f="" n="1">at the computer-services concern</arg>
                
                <note/>
            </example>
            
            <example name="Another arg 1 as procede" src="" type="">
                <text>you 're going to make a run of it</text>
                
                
                <rel f="">run</rel>
                <arg f="" n="1">of it</arg>
                
                <note/>
