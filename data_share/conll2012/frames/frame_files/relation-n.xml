<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
    <predicate lemma="relation">
        <roleset framnet="" id="relation.01" name="to know an entity" vncls="">
            <roles>
                
                <role descr="first party" f="" n="0"/>
                
                <role descr="type of relationship/ reason to know" f="" n="1"/>
                
                <role descr="second party/ relations with/ beneficiary" f="" n="2"/>
                
                
                <note/>
            </roles>
            
            <example name="Example with arg 1 and 2" src="" type="">
                <text>South Korea established diplomatic relations with Hungary in February 1989</text>
                
                
                <arg f="" n="1">diplomatic</arg>
                <rel f="">relations</rel>
                <arg f="" n="2">with Hungary</arg>
                
                <note/>
            </example>
            
            <example name="Example with arg 0 and 2" src="" type="">
                <text>our relations with Japan</text>
                <arg f="" n="0">our</arg>
                <rel f="">relations</rel>
                <arg f="" n="2">with Japan</arg>
                
                <note/>
            </example>
            
            
            <note>No corresponding verbnet class. Framed by Katie.</note>
        </roleset>
        <roleset framnet="" id="relation.02" name="be related to" source="verb-relate.01" vncls="">
            <roles>
                
                <role descr="agent, entity drawing comparison" f="" n="0"/>
                
                <role descr="thing under consideration" f="" n="1"/>
                
                <role descr="what it's related to" f="" n="2"/>
                
                
                <note/>
            </roles>
            
            <example name="Very common 'in relation to'" src="" type="">
                <text>low prices in relation to the company 's earnings per share</text>
                
                <rel f="">relation</rel>
                <arg f="" n="2">to the company 's earnings per share</arg>
                
                <note/>
            </example>
            
            <example name="Family relation" src="" type="">
                <text>Ed Turner, no relation to Ted Turner -RRB-</text>
                <arg f="" n="1">Ed Turner,</arg>
                <arg f="neg" n="m">no</arg>
                <rel f="">relation</rel>
                <arg f="" n="2">to Ted Turner</arg>
                
                <note/>
            </example>
            
            
            <example name="What to do when the things being compared are in the same node" src="" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>The relation between physical education and the economy</text>
                
                <rel f="">relation</rel>
                <arg f="" n="1">between physical education and the economy</arg>
                <note/>
            </example>
            <note>Roleset based on relate.01. No corresponding verbnet class. Framed by Katie</note>
        </roleset>
        
        <note/>
    </predicate>
    
    <note/>
</frameset>
