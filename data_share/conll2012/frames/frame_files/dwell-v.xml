        <roleset id="dwell.01" vncls="46 47.1-1" name="reside, linger">
            <roles>
                <role n="0"  descr="dweller, inhabitant, entity in discourse">
                    <vnrole
  vncls="46" vntheta="Theme"/><vnrole vncls="47.1-1" vntheta="Theme"/>
