        <roleset framnet="-" id="snap.01" name="bite at viciously, perhaps as an utterance" vncls="37.3">
            <roles>
                <role descr="speaker, snapper" f="" n="0">
                    
                    <vnrole vncls="37.3" vntheta="agent"/>
                </role>
                <role descr="utterance" f="" n="1">
                    
                    <vnrole vncls="37.3" vntheta="topic"/>
                </role>
                <role descr="hearer, entity being snapped at" f="gol" n="2">
                    
