<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
    <predicate lemma="itemize">
        
        
        <roleset framnet="-" id="itemize.01" name="list each member of a group" vncls="-">
            <roles>
                <role descr="lister" f="" n="0"/>
                <role descr="items" f="" n="1"/>
                <note/>
            </roles>
            
            <example name="one example" src="" type="">
                <inflection aspect="ns" form="full" person="ns" tense="present" voice="active"/>
                <text>Charitable groups are trying to reinstate the write-off for
                    contributions made by individuals who-1 [*T*-1] don't itemize their
