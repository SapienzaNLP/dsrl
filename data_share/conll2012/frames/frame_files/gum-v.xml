    <predicate lemma="gum">
        <note/>
        <roleset framnet="" id="gum.01" name="Fill, attach, or fix with gum or gum-like substance" vncls="22.4-1">
            <roles>
                <role descr="Fixer" f="" n="0">
                    <vnrole vncls="22.4-1" vntheta="agent"/>
                </role>
                <note/>
                <role descr="First (or all in one constituent) thing gummed" f="" n="1">
                    <vnrole vncls="22.4-1" vntheta="patient1"/>
                </role>
                <role descr="Thing arg1 is attached to with gum (when separate constituent)" f="" n="2">
                    <vnrole vncls="22.4-1" vntheta="patient2"/>
                </role>
                <role descr="Gum itself, type of gum used, instrument" f="" n="3">
                    <vnrole vncls="22.4-1" vntheta="instrument"/>
                </role>
            </roles>
            <note>Roleset based on parallel additions to VN, PB and sense groupings. VN class 22.4-1.  Framed by Claire.</note>
            <example name="No instrument, type of gum mentioned" src="web" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>[I]-1 sewed the glasses on, and *PRO*-1 gummed the mustache to my face.</text>
                <note/>
                <arg f="" n="0">*PRO*-1</arg>
                <rel f="">gummed</rel>
                <arg f="" n="1">the mustache</arg>
                <arg f="" n="2">to my face</arg>
            </example>
            <example name="With PP &quot;on&quot;" src="web" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>The buds looked as if [they]-1 were gummed *-1 on to the lower side of the leaves.</text>
                <note/>
                <rel f="">gummed</rel>
                <arg f="" n="1">*-1</arg>
                <arg f="" n="2">on to the lower side of the leaves</arg>
            </example>
            <example name="All arguments, invented example" src="web" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>John gummed the handle to the toaster oven with cherry Big-League chew.</text>
                <note/>
                <arg f="" n="0">John</arg>
                <rel f="">gummed</rel>
                <arg f="" n="1">the handle</arg>
                <arg f="" n="2">to the toaster oven</arg>
                <arg f="" n="3">with cherry Big-League chew.</arg>
            </example>
        </roleset>
        <roleset framnet="" id="gum.02" name="To chew without gums" vncls="39.2-1">
            <roles>
                <role descr="chewer" f="" n="0">
                    <vnrole vncls="39.2-1" vntheta="agent"/>
                </role>
                <note/>
                <role descr="food" f="" n="1">
                    <vnrole vncls="39.2-1" vntheta="patient"/>
                </role>
            </roles>
            <note>Roleset based on parallel additions to VN, PB and sense groupings. Framed by Claire. VN entry chew-39.2-1.</note>
            <example name="With secondary predication" src="web" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>She gummed the apple to death.</text>
                <note/>
                <arg f="" n="0">She</arg>
                <rel f="">gummed</rel>
                <arg f="" n="1">the apple</arg>
                <arg f="prd" n="m">to death</arg>
            </example>
            <example name="Connative" src="web" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>The old man gummed at his wine-cup.</text>
                <note/>
                <arg f="" n="0">The old man</arg>
                <rel f="">gummed</rel>
                <arg f="" n="1">at his wine-cup</arg>
            </example>
        </roleset>
