            <example name="sign to" src="" type="">
                <text>John was signed to the Mudville Giants.</text>
                <arg f="" n="2">John</arg>
                <rel f="">signed</rel>
                <arg f="" n="1">to the Mudville Giants</arg>
                
                
                
