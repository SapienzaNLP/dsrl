                <role descr="entity being taken over" f="" n="1"/>
                
                
                <note/>
            </roles>
            
            <example name="example with both args" src="" type="">
                <text>its takeover by Coniston Partners</text>
                <arg f="" n="1">its</arg>
                <rel f="">takeover</rel>
                <arg f="" n="0">by Coniston Partners</arg>
                
                <note/>
            </example>
            
            <example name="Example with only arg 1" src="" type="">
                <text>the UAL takeover</text>
                <arg f="" n="1">UAL</arg>
                <rel f="">takeover</rel>
                
                <note/>
            </example>
            
            
            <example name="Example with both args where arg 0 may be confusing" src="" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>An opposition takeover of the company</text>
                
                <arg f="" n="0">An opposition</arg>
                <rel f="">takeover</rel>
                <arg f="" n="1">of the company</arg>
                <note/>
            </example>
            <example name="Make sure you know whether an arg is the thing taking over or the thing begin taken over" src="" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>The WWP takeover</text>
                
                <arg f="" n="0">The WWP</arg>
                <rel f="">takeover</rel>
