                <arg f="" n="1">US jobless claims</arg>
                <rel f="">tick</rel>
                <arg f="dir" n="m">down</arg>
                <arg f="" n="3">to 2008 levels.</arg>
            </example>
            <example name="arg 0 may be rare" src="" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>2011 crime stats tick up</text>
                <note/>
                <arg f="" n="1">2011 crime stats</arg>
                <rel f="">tick</rel>
                <arg f="dir" n="m">up</arg>
            </example>
