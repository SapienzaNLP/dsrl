<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
    <predicate lemma="sling">
        
        <roleset framnet="-" id="sling.01" name="sling" vncls="17.1  9.1">
            <roles>
                <role descr="slinger" f="" n="0">
                    <vnrole vncls="17.1" vntheta="agent"/>
                    <vnrole vncls="9.1" vntheta="agent"/>
                </role>
                <role descr="thing slung" f="" n="1">
                    <vnrole vncls="17.1" vntheta="theme"/>
                    <vnrole vncls="9.1" vntheta="theme"/>
                </role>
                <role descr="target" f="gol" n="2">
                    <vnrole vncls="17.1" vntheta="destination"/>
                    <vnrole vncls="9.1" vntheta="destination"/>
