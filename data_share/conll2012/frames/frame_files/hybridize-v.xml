                <arg n="M" f="NEG">not</arg>
                <rel>hybridize</rel>
                <arg n="2">with each other</arg>
                <arg f="MNR" n="M">under high stringency conditions</arg>
            </example>
            
            
        </roleset>
    </predicate>
    
