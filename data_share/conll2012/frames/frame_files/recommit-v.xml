                <rel f="">recommit</rel>
                <arg f="tmp" n="m">until they do it.</arg>
            </example>
            <example name="commit again to a committee" src="" type="">
                <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
                <text>The bill was recommitted three times in the house.</text>
                <note/>
